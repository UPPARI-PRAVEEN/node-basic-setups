const mongoose = require('mongoose'); // Don't forget to import mongoose

// Connect to MongoDB
mongoose
    .connect(process.env.MONGOOSEDB_RUL, { useNewUrlParser: true, useUnifiedTopology: true })
    .then(() => console.log('Connected to MongoDB yessss'))
    .catch((err) => console.error('Error connecting to MongoDB:', err));


    //all this files import on index.js
